{
  "name": "migrations",
  "description": "Command line migrations for knex.js with typescript power by oclif.",
  "version": "0.0.0",
  "author": {
    "name": "Supakit Tanyung",
    "email": "supakit.tanyung@gmail.com",
    "url": "https://github.com/MunNaaS"
  },
  "bin": {
    "migrations": "./bin/run"
  },
  "bugs": "https://github.com/MunNaaS/migrations/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^1",
    "lodash": "^4.17.10",
    "oclif": "^1.7.40"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@types/chai": "^4",
    "@types/knex": "^0.14.14",
    "@types/lodash": "^4.14.108",
    "@types/mocha": "^5",
    "@types/node": "^9",
    "backpack-core": "^0.7.0",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^11",
    "ts-node": "^6",
    "tslib": "^1",
    "tslint": "^5",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8"
  },
  "peerDependencies": {
    "knex": "^0.14.6"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/MunNaaS/migrations",
  "keywords": [
    "oclif", "knex", "migrations", "migrate", "typescript", "database"
  ],
  "license": "MIT",
  "main": "index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "migrations",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "private": false,
  "repository": "MunNaaS/migrations",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "tslint -p .",
    "fix": "tslint --fix -p ."
  },
  "types": "lib/index.d.ts"
}
