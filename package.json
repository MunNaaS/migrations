{
  "name": "@munnaas/migrations",
  "description": "Command line migrations for knex.js with typescript power by oclif.",
  "version": "0.0.0",
  "author": "Supakit Tanyung <supakit.tanyung@gmail.com> (https://github.com/MunNaaS)",
  "bin": {
    "migrations": "./bin/run"
  },
  "bugs": "https://github.com/MunNaaS/migrations/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^1",
    "change-case": "^3.0.2",
    "inquirer": "^5.2.0",
    "listr": "^0.14.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "oclif": "^1.7.40"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@types/chai": "^4",
    "@types/globby": "^6.1.0",
    "@types/inquirer": "^0.0.41",
    "@types/knex": "^0.14.14",
    "@types/listr": "^0.13.0",
    "@types/lodash": "^4.14.108",
    "@types/mocha": "^5",
    "@types/node": "^10.0.8",
    "@types/source-map-support": "^0.4.0",
    "chai": "^4",
    "globby": "^8.0.1",
    "mocha": "^5",
    "nyc": "^11",
    "source-map-support": "^0.5.5",
    "ts-node": "^6",
    "tslib": "^1",
    "tslint": "^5",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8"
  },
  "peerDependencies": {
    "knex": "^0.14.6"
  },
  "engines": {
    "node": ">=8.11.1"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/MunNaaS/migrations",
  "keywords": [
    "oclif",
    "knex",
    "migrations",
    "migrate",
    "typescript",
    "database"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/Commands",
    "bin": "migrations",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/MunNaaS/migrations",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "tslint -p .",
    "fix": "tslint --fix -p ."
  },
  "types": "lib/index.d.ts"
}
