{
  "name": "migrations",
  "version": "1.0.0",
  "author": "supakit @MunNaaS",
  "bin": {
    "migrations": "./bin/run"
  },
  "bugs": "https://github.com/MunNaaS/migrations/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^1",
    "oclif": "^1.7.40"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@types/chai": "^4",
    "@types/knex": "^0.14.14",
    "@types/mocha": "^5",
    "@types/node": "^9",
    "backpack-core": "^0.7.0",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^11",
    "ts-node": "^6",
    "tslib": "^1",
    "tslint": "^5",
    "typescript": "^2.8"
  },
  "peerDependencies": {
    "knex": "^0.14.6"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/MunNaaS/migrations",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "migrations",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "private": true,
  "repository": "MunNaaS/migrations",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
